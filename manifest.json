{
    "manifest_version": 2,
    "name": "YoutubeTranscription",
    "description": "open youtube transciption with shortcut key",
    "version": "1",
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/watch*"
            ],
            "js": [
                "./src/main.js"
            ]
        }
    ]
}